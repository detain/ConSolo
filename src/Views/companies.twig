{% extends "shared/emubase.twig" %}

{% block content %}
<table class="table table-striped table-bordered data-table" style="width:100%">
    <thead>
        <tr>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
{% for data in results %}
        <tr>
            <td><a href="{{ app.request.basePath }}/company/{{ data.id }}">{{ data.name }}</a></td>
        </tr>
{% endfor %}
    </tbody>
</table>
{% endblock %}
